target 'Flashcard' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Flashcard
	pod 'RealmSwift'

  target 'FlashcardTests' do
    inherit! :search_paths
    # Pods for testing
	pod 'RealmSwift'
  end

  target 'FlashcardUITests' do
    inherit! :search_paths
    # Pods for testing
	pod 'RealmSwift'
  end

end
